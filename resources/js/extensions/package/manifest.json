{
  "name": "VPS Extention",
  "version": "1.0",
  "manifest_version": 3,
  "description": "Google extension for VPS. By Tho.",
  "icons": {
    "128": "icon.png"
  },
  "content_scripts": [
    {
      "matches": [
        "https://smartpro.vps.com.vn/v1/"
      ],
      "js": [
        "js/moment.min.js",
        "js/lightweight-charts.min.js",
        "js/simple-notify.min.js",
        "js/google-protobuf.js",
        "js/tcbs.js",
        "js/main.js"
      ],
      "css": [
        "css/simple-notify.min.css",
        "css/main.css"
      ],
      "run_at": "document_idle",
      "all_frames": false
    }
  ],
  "web_accessible_resources": [
    {
      "resources": [
        "config.json",
        "spinner.gif",
        "alert.wav"
      ],
      "matches": [
        "https://smartpro.vps.com.vn/*"
      ]
    }
  ]
}