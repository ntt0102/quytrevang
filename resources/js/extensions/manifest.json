{
  "name": "VPS Extention",
  "version": "1.0",
  "manifest_version": 3,
  "description": "Google extension for VPS. By Tho.",
  "icons": {
    "128": "icon.png"
  },
  "content_scripts": [
    {
      "matches": [
        "https://smartpro.vps.com.vn/v1/"
      ],
      "js": [
        "smartpro/socket.io.js",
        "smartpro/moment.min.js",
        "smartpro/hammer.min.js",
        "smartpro/chart.min.js",
        "smartpro/chartjs-plugin-zoom.min.js",
        "smartpro/chartjs-adapter-moment.min.js",
        "smartpro/script.js"
      ],
      "css": [
        "smartpro/style.css"
      ],
      "run_at": "document_idle",
      "all_frames": false
    }
  ],
  "web_accessible_resources": [
    {
      "resources": [
        "config.json"
      ],
      "matches": [
        "https://smartpro.vps.com.vn/*"
      ]
    }
  ]  
}